{
    "name": "popphp/pop-audit",
    "description": "Pop Audit Component for Pop PHP Framework",
    "keywords": [
        "audit",
        "auditing",
        "application auditing",
        "php audit",
        "php auditing",
        "php",
        "pop",
        "pop php"
    ],
    "homepage": "http://github.com/popphp/pop-audit",
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Nick Sagona",
            "email": "dev@nolainteractive.com",
            "homepage": "http://www.nolainteractive.com/"
        }
    ],
    "require": {
        "php": ">=7.0.0",
        "popphp/popphp": "^3.2.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.5.0",
        "popphp/pop-db": "^4.3.1",
        "popphp/pop-http": "^3.0.0"
    },
    "autoload": {
        "psr-4": {
            "Pop\\Audit\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Pop\\Audit\\Test\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
