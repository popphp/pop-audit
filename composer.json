{
    "name": "popphp/pop-audit",
    "description": "Pop Audit Component for Pop PHP Framework",
    "keywords": [
        "audit",
        "auditing",
        "application auditing",
        "php audit",
        "php auditing",
        "php",
        "pop",
        "pop php"
    ],
    "homepage": "http://github.com/popphp/pop-audit",
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Nick Sagona",
            "email": "dev@noladev.com",
            "homepage": "https://www.popphp.org/"
        }
    ],
    "require": {
        "php": ">=8.3.0",
        "popphp/popphp": "^4.4.0",
        "popphp/pop-db": "^6.7.0",
        "popphp/pop-http": "^5.3.8"
    },
    "require-dev": {
        "phpunit/phpunit": "^12.0.0"
    },
    "autoload": {
        "psr-4": {
            "Pop\\Audit\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Pop\\Audit\\Test\\": "tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.0.x-dev"
        }
    }
